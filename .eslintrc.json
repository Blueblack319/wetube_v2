{
  "env": {
    "browser": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"],
  "extends": [
    "plugin:prettier/recommended",
    "some-other-config-you-use",
    "prettier"
  ]
}
